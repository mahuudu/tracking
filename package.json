{
  "name": "@mahuudu/tracking",
  "version": "1.0.2",
  "description": "Browser-based tracking framework for Next.js/React and vanilla JS with UTM tracking, event tracking, attribution, page value, and funnel analytics",
  "keywords": [
    "tracking",
    "analytics",
    "attribution",
    "funnel",
    "utm",
    "utm-tracking",
    "marketing-analytics",
    "pageview",
    "event-tracking",
    "react",
    "nextjs",
    "typescript"
  ],
  "author": "mahuudu",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mahuudu/tracking"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:minify": "npm run build && node scripts/minify.js",
    "prepublishOnly": "npm run build:minify",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "esbuild": "^0.27.2",
    "react": "^18.0.0",
    "typescript": "^5.0.0"
  }
}
